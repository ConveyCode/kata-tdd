{
    "name": "eres-group/dev-training",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "~8.2",
        "ext-ctype": "*",
        "ext-iconv": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "symfony/var-dumper": "6.3.*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Cupcake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cupcake\\Tests\\": "tests/"
        }
    }
}
